<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <arg name="marker_initial_x" default="5.0"/>
  <arg name="marker_initial_y" default="-0.6"/>

  <!-- add_markers node -->
  <node pkg="add_markers" type="add_markers_node" name="add_markers" output="screen">
    <param name="initial_x" type="double" value="$(arg marker_initial_x)"/>
    <param name="initial_y" type="double" value="$(arg marker_initial_y)"/>
  </node>

</launch>
